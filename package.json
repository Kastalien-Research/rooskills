    "!.roo/skills/scripts/**/*.pyc",
    "!.roo/skills/scripts/**/venv",
    "coding-agent-docs/",
    ".roomodes",
    ".env.example",
    "agent_skills_spec.md",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
