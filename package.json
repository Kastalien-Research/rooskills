  "keywords": [
    "roo-code",
    "windsurf",
    "claude",
    "anthropic",
    "agent-skills",
    "ai",
    "cli",
    "automation",
    "coding-assistant",
    "developer-tools",
    "llm",
    "prompt-engineering",
    "code-generation"
  ],
  "author": "Kastalien Research",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kastalien-Research/rooskills.git"
  },
  "bugs": {
    "url": "https://github.com/Kastalien-Research/rooskills/issues"
  },
  "homepage": "https://github.com/Kastalien-Research/rooskills#readme",
  "files": [
